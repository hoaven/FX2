.i_top_news{ padding:0 10px; height:24px; text-align:center;background-color:#FFF8B6; overflow:hidden;position:relative;}
.i_top_news span{ font-size:0.7em; color:#FA7615;line-height:24px;}
.i_top_news a{display:inline-block;padding:1px 6px;font-size:0.6em;}
/*—————————输入新增—————————*/
.addform{overflow:hidden; background-color:transparent;}
.addPhoto{ padding:8px; margin-bottom:10px;background:#FFF;border-top:1px solid #efeff4;border-bottom:1px solid #efeff4; text-align:center;}
.addPhoto .addFile{ position:absolute;width:80px; height:72px; margin:0; padding:0;opacity:0;}
.addPhoto span{ display:block; margin:0 auto; border:1px solid #B4B4B4; background-color:#F5F5F5; box-shadow:0 0 2px #CDCDCD; color:#B8B8B8; width:80px;height:72px; line-height:72px;}
.addform table{ width:100%; margin-bottom:10px;background:#FFF; }
.addform table td{ padding:12px 20px 12px 0;vertical-align:top; line-height:22px;border-top:1px solid #efeff4; border-bottom:1px solid #efeff4;}
.addform table td.TDleft{ width:40px; white-space:nowrap;padding-right:0; padding-left:20px;}
.addform table td.TDleft b{color: #ff0000}
.addform table td.padding-left{padding-left:20px;}
.nlinput{text-align:left; color:#8C8C8C;}
.nlselect{  border:1px solid #B4B4B4;text-align:center;}
.nltextarea{border:1px solid #ABADB3; overflow:hidden; box-shadow: inset 1px 1px 1px #DDD; height:80px;}
.addform .nl-select select{ width:100%; border:0;background:transparent;text-align:center;}
.addform .nl-input input[type="text"]{ text-align:center;}
.addform input.Winp01{border:1px solid #B4B4B4;width: 180px}
.addform input.Winp02{ width:23px;border:1px solid #B4B4B4; }
.nlchecked label{float:left; width:33.3333%;}
.nlchecked02 label{float:left; width:25%;}
.addform_lable{ padding:4px 20px; color:#2974FF; font-size:0.7em;}
.addform .codeSource{ display:block;width:90px;color:#888888;  text-align:center;background-color:#FFFFFF;border: 1px solid #B4B4B4;box-shadow: inset 1px 1px 1px #DDD; }
.addform .codeSource:active,.addform .codeSource:focus { background-color:#ebebeb; color:#A9A9A9;}
/*—————————房源—————————*/
.show_main_card{ padding:10px 20px; border-top:1px solid #C6C6C6; border-bottom:1px solid #C6C6C6;}
.padding-bot{ padding:0 10px 0 0;}
.show_title_card{ padding:20px 10px 20px 20px; border-bottom:1px solid #C6C6C6;}
.slide-box{display: block;width: 100%;overflow: hidden;position: relative;background: #000;}
.slide-box img {min-width: 100%}
.show_title{ line-height:26px;}
.show_title h3{}
.show_title h4{ color:#6D6D6D; font-size:0.9em;}
.show_title_op{ width:95px; line-height:22px;  font-size:0.8em; text-align:center;}
.show_title_op a.bGreen{ background-color:#0FC527; border:1px solid #A4DEAB; height:22px;display:block;text-align: center}
.show_title_op span{ color:#A4A4A4;}
.show_cont li{ padding:5px 0; line-height:28px;}
.show_cont li .TDleft{ width:75px;vertical-align:top;}
.show_cont li .price B{color:#DC0000;font-weight:normal;}
.show_cont li .show_bj{ width:85px; text-align:right;}
.show_cont li .bRed{ padding:3px 5px; font-size:0.8em}
/*标签块*/
.box_ss {clear:both;overflow:hidden;}
.box_ss li{ padding:0;  float:left; width:25%;}
.box_ss span{display:block; margin:3px 3px 3px 0; box-shadow:0 0 2px #CDCDCD;background-color:#F4F4F4; border:1px solid #B4B4B4;color:#7F7F7F;font-size:13px; text-align:center; height:21px; line-height:21px; overflow:hidden;}
.box_ss span i{display:block; box-shadow: inset 0 0 2px #FFF; font-style:normal;}
.box_ss .on{ background-color:#0FC527; border:1px solid #62FF75; color:#FFF;}
.box_label {clear:both;overflow:hidden;}
.box_label li{padding:0;float:left; width:33.3333%;}
.box_label span{display:block; margin-right:10px; background-color:#ECECEC; border:1px solid #B4B4B4;color:#7F7F7F;text-align:center; font-size:14px; }
.box_label .on{ background-color:#0FC527; border:1px solid #A4DEAB; color:#FFF;}
.box_label span:nth-child(4n),.box_label span:nth-child(3n) { margin-right:0;}
